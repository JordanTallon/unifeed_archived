pipeline {
    // Run on the next available agent
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building the Unifeed application..'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing the application..'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying the application..'
            }
        }
    }
}
